                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <BorderPane.margin>
                    <Insets bottom="100.0" top="100.0"/>
                </BorderPane.margin>
                <JFXButton fx:id="bVentas" alignment="BOTTOM_CENTER" buttonType="RAISED" contentDisplay="CENTER"
