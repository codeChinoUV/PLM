                           onAction="#regresarAVentanaPrincipal" prefHeight="180.0" prefWidth="313.0"
                           ripplerFill="WHITE" style="-fx-background-color: #F14634;" text="Regresar"
                           textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="2">
                    <font>
                        <Font size="15.0"/>
                    </font>
                    <padding>
                        <Insets right="5.0"/>
                    </padding>
                </JFXButton>
                <ImageView fitHeight="148.0" fitWidth="141.0" onMouseClicked="#regresarAVentanaPrincipal"
                           pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
                    <GridPane.margin>
                        <Insets left="85.0"/>
                    </GridPane.margin>
                    <Image url="@../img/regresar.png"/>
                </ImageView>
            </GridPane>
        </center>
        <left>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
        </left>
        <right>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
        </right>
    </BorderPane>
</AnchorPane>
