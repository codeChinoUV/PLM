                <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                    <Image url="@../img/logo.png"/>
                </ImageView>
                <Label prefHeight="160.0" prefWidth="369.0" text="Menu inventario" textAlignment="CENTER"
                       textFill="WHITE">
                    <font>
                        <Font name="System Bold Italic" size="36.0"/>
                    </font>
                </Label>
                <GridPane prefHeight="150.0" prefWidth="845.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
                        <Label prefHeight="83.0" prefWidth="79.0" text="Usuario:" textFill="WHITE">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Label>
                        <Label fx:id="lUsuario" prefHeight="82.0" prefWidth="318.0" textFill="WHITE">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <HBox.margin>
                                <Insets left="20.0"/>
                            </HBox.margin>
                        </Label>
                    </HBox>
                    <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                        <Label prefHeight="83.0" prefWidth="61.0" text="Fecha:" textFill="WHITE">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Label>
                        <Label fx:id="lFecha" prefHeight="84.0" prefWidth="355.0" textFill="WHITE">
                            <font>
                                <Font size="20.0"/>
                            </font>
                            <padding>
                                <Insets left="20.0"/>
                            </padding>
                        </Label>
                    </HBox>
                </GridPane>
            </HBox>
        </top>
        <center>
            <GridPane prefHeight="458.0" prefWidth="1228.0" BorderPane.alignment="CENTER">
