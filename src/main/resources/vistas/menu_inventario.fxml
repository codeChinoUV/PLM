                        <Font size="15.0"/>
                    </font>
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                    <padding>
                        <Insets right="5.0"/>
                    </padding>
                </JFXButton>
                <ImageView fitHeight="133.0" fitWidth="151.0" onMouseClicked="#mostrarVentanaAdministrarInventario"
                           pickOnBounds="true" preserveRatio="true">
                    <GridPane.margin>
                        <Insets left="90.0"/>
                    </GridPane.margin>
                    <Image url="@../img/administrar_inventario.png"/>
                </ImageView>
                <JFXButton alignment="BOTTOM_CENTER" buttonType="RAISED" mnemonicParsing="false" prefHeight="179.0"
                           prefWidth="306.0" style="-fx-background-color: #F0741E;" text="Reportes de inventario"
                           textAlignment="CENTER" textFill="WHITE" GridPane.columnIndex="1">
                    <font>
                        <Font size="15.0"/>
                    </font>
                </JFXButton>
                <ImageView fitHeight="132.0" fitWidth="179.0" pickOnBounds="true" preserveRatio="true"
                           GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets left="89.0"/>
                    </GridPane.margin>
                    <Image url="@../img/reportes.png"/>
                </ImageView>
                <JFXButton fx:id="bRegresar" alignment="BOTTOM_CENTER" buttonType="RAISED" contentDisplay="CENTER"
